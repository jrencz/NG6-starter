describe('{{ dashCase name }} Route', () => {

  // Before each test
  beforeEach(() => {

    // Navigate to {{ camelCase name }} component
    browser.get('/{{ dashCase name }}');
  });

  // {{ camelCase name }} component should be visible
  it('should be visible', () => {

    // Expect {{ camelCase name }} component to be visible
    expect(element(by.tagName('{{ dashCase name }}')).isDisplayed()).toBe(true);
  });

  // Link to Home navigates to Home Component
  it('should navigate to Home if Home link clicked', () => {

    // Click navigation to Home route
    element(by.css('[ui-sref="home"]'))
      .click()
      .then(() => {

        // Expect {{ camelCase name }} component to be not be present
        expect(element(by.tagName('{{ dashCase name }}')).isPresent()).toBe(false);
      })
  });

});
