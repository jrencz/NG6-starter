{
  "name": "ng6-starter",
  "version": "0.0.1",
  "description": "Starter for Angular + ES6 + (Webpack or JSPM)",
  "main": "index.js",
  "config": {
    "port": 3000,
    "browserslistStats": "./node_modules/stats/stats.json",
    "localdev": "localdev.config.json"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-ui-router": "^1.0.0-beta.1",
    "test-standalone-component": "git@github.com:jrencz/webpack-library-starter.git#stand-alone-component-test",
    "test-theme": "git@github.com:jrencz/test-documentcss.git",
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.1",
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "browser-sync": "^2.18.5",
    "builtin-modules": "^1.1.1",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eclint": "^1.1.5",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-angular": "^1.6.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-protractor": "^1.32.0",
    "eslint-plugin-spellcheck": "0.0.8",
    "fs-utils": "^0.7.0",
    "fs-walk": "0.0.1",
    "glob": "^7.1.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.25.0",
    "inquirer-directory": "^2.1.0",
    "json5": "^0.5.1",
    "jsonfile": "^2.4.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-eslint": "^2.1.0",
    "karma-longest-reporter": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "lintspaces": "^0.5.0",
    "lintspaces-cli": "github:jrencz/lintspaces-cli#dotfiles",
    "localdev": "git@github.com:jrencz/localdev.git",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "ng-annotate-loader": "0.2.0",
    "node-fetch": "^1.6.3",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.1.1",
    "npm-check-updates": "^2.8.9",
    "npmls": "^3.0.0",
    "plop": "^1.7.3",
    "postcss-load-config": "^1.0.0",
    "postcss-loader": "^1.2.1",
    "protractor": "^4.0.14",
    "raw-loader": "^0.5.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.1.1",
    "stats": "gist:60a2135fca54e6cfa4b367fbb7d39639",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-scss": "^1.4.1",
    "stylelint-webpack-plugin": "^0.4.2",
    "supports-color": "^3.1.2",
    "twig-loader": "^0.2.4",
    "uri-templates": "^0.2.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "yargs": "^6.6.0"
  },
  "scripts": {
    "postinstall": "npm run maintenance:locallink",
    "start": "export BROWSERSLIST_STATS=$npm_package_config_browserslistStats; browser-sync start --config bs-config.js & npm run maintenance:localstart",
    "build": "export BROWSERSLIST_STATS=$npm_package_config_browserslistStats; gulp clean && webpack --config=webpack.dist.config.js",
    "generate": "plop",
    "generate:component": "plop component",
    "serve": "browser-sync start --config bs-config.js",
    "test": "npm run lint && karma start && protractor protractor.conf.js",
    "test:smoke": "npm run test:unit:smoke && rpm run test:e2e:smoke",
    "test:unit": "karma start",
    "test:unit:smoke": "karma start --suite=\"spec-smoke.bundle.js\"",
    "test:unit:dev": "karma start --auto-watch --no-single-run --reporters progress",
    "test:e2e:smoke": "protractor protractor.conf.js",
    "test:e2e:all": "protractor protractor.conf.js --suite=\"all\"",
    "lint": "npm run lint:source && npm run lint:specs && npm run lint:e2e && npm run lint:styles && npm run lint:editorconfig",
    "lint:source": "eslint \"**/*.js\" --ignore-pattern=\"*.spec.js\" --ignore-pattern=\"*.e2e.js\"",
    "lint:specs": "eslint \"**/*.spec.js\" --config=\"./.eslintrc-spec.js\"",
    "lint:e2e": "eslint \"**/*.e2e.js\" --config=\"./.eslintrc-e2e.js\"",
    "lint:styles": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:editorconfig": "lintspaces --editorconfig .editorconfig --ignores \"js-comments\" --matchdotfiles \"./!(node_modules|dist|.tmp|.git|.idea)/**/!(.DS_Store)*\" \"./!(.DS_Store|LICENSE)*\"",
    "maintenance:locallink": "localdev-link --config=\"$npm_package_config_localdev\"",
    "maintenance:localstart": "localdev-start --config=\"$npm_package_config_localdev\"",
    "maintenance:webdriver-update": "webdriver-manager update",
    "dependencies:check-updates": "ncu",
    "dependencies:update": "ncu -a && npm update",
    "info:supported-browsers": "BROWSERSLIST_STATS=$npm_package_config_browserslistStats browserslist --config=browserslist"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/angularclass/NG6-starter.git"
  },
  "author": "AngularClass",
  "license": "Apache-2.0"
}
